local e = Instance.new("ScreenGui")
local Killstreak = Instance.new("TextLabel")
local reaper = Instance.new("TextLabel")
local reaper_2 = Instance.new("TextLabel")

--Properties:

e.Name = "e"
e.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
e.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Killstreak.Name = "Killstreak"
Killstreak.Parent = e
Killstreak.BackgroundColor3 = Color3.fromRGB(25, 144, 255)
Killstreak.BorderSizePixel = 5
Killstreak.Position = UDim2.new(0, 0, 0.332155466, 0)
Killstreak.Size = UDim2.new(0.182972342, 0, 0.0565370768, 0)
Killstreak.Font = Enum.Font.Gotham
Killstreak.Text = "PRESS E TO ACTIVATE KILL STREAK AURA. MUST HAVE IT EQUIPPED"
Killstreak.TextColor3 = Color3.fromRGB(0, 0, 0)
Killstreak.TextScaled = true
Killstreak.TextSize = 14.000
Killstreak.TextWrapped = true

reaper.Name = "reaper"
reaper.Parent = e
reaper.BackgroundColor3 = Color3.fromRGB(25, 144, 255)
reaper.BorderSizePixel = 5
reaper.Position = UDim2.new(0, 0, 0.425795048, 0)
reaper.Size = UDim2.new(0.182972342, 0, 0.0618374571, 0)
reaper.Font = Enum.Font.Gotham
reaper.Text = "PRESS P TO ACTIVATE THE REAPER AURA. MUST HAVE IT EQUIPPED."
reaper.TextColor3 = Color3.fromRGB(0, 0, 0)
reaper.TextScaled = true
reaper.TextSize = 14.000
reaper.TextWrapped = true

reaper_2.Name = "reaper"
