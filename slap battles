-- Gui to Lua
-- Version: 3.2

-- Instances:

local e = Instance.new("ScreenGui")
local Killstreak = Instance.new("TextLabel")
local reaper = Instance.new("TextLabel")
local boomerang = Instance.new("TextLabel")
local extendedhand = Instance.new("TextLabel")
