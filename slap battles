-- Scripts:

local function RSCHG_fake_script() -- e.Userinputservice 
	local script = Instance.new('LocalScript', e)

	local userinputservice = game:GetService("UserInputService")
	getgenv().Enabled = true 
	getgenv().EnabledReaper = true
	getgenv().EnabledBoomerang = true 
	getgenv().EnabledExtended = true 
	getgenv().EnabledBull = true 
	local players = game:GetService("Players")
	function killstreak(input, IsTyping) 
		if IsTyping == true then return end 
		if input.KeyCode == Enum.KeyCode.E then 
			print("Pressed E")
			while getgenv().Enabled == true do 
				if getgenv().Enabled == true then 
					task.wait()
					local allplayers = players:GetPlayers() 
					for i, v in pairs(allplayers) do 
						task.wait()
						if v:IsA("Player") then 
							local name = v.Name 
							local args = {[1] = players[v.Name].Character:FindFirstChild("Right Arm")}
							game:GetService("ReplicatedStorage"):WaitForChild("KSHit"):FireServer(unpack(args))
						end 
					end 
				end 
			end 
		end
	end
	
	userinputservice.InputBegan:Connect(killstreak)
	function reaper(input, isTyping)
		if isTyping == true then return end 
		if input.KeyCode == Enum.KeyCode.P then 
			print("Pressed P")
			while getgenv().EnabledReaper == true do 
				if getgenv().EnabledReaper == true then 
					task.wait()
					local all = players:GetPlayers() 
					for i, v in pairs(all) do 
						task.wait()
						if v:IsA("Player") then 
							local name = v.Name 
							local args = {[1] = players[v.Name].Character:FindFirstChild("Right Arm")}
							game:GetService("ReplicatedStorage"):WaitForChild("ReaperHit"):FireServer(unpack(args))
						end 
					end 
				end 
			end 
		end
	end
	userinputservice.InputBegan:Connect(reaper)
	function boomerang(input, isTyping)
		if isTyping == true then return end 
		if input.KeyCode == Enum.KeyCode.N then 
			print("Pressed N")
			while getgenv().EnabledBoomerang == true do 
				task.wait()
				if getgenv().EnabledBoomerang == true then 
					local a = players:GetPlayers()
					for i, v in pairs(a) do 
						task.wait()
						local name = v.Name 
						local args = {[1] = players[v.Name].Character:FindFirstChild("Right Arm")}
						game:GetService("ReplicatedStorage"):WaitForChild("BoomerangH"):FireServer(unpack(args))
	
					end 
				end
			end 
		end
	end
	userinputservice.InputBegan:Connect(boomerang)
	
	function extendedhand(input, istyping)
		if istyping == true then return end 
		if input.KeyCode == Enum.KeyCode.L then
			print("Pressed L")
			while getgenv().EnabledExtended == true do 
				task.wait() 
				if getgenv().EnabledExtended == true then 
					local zeeplayers = players:GetPlayers() 
					for i, v in pairs(zeeplayers) do
						task.wait() 
						local name = v.Name 
						local args = {[1] = players[v.Name].Character:FindFirstChild("Right Arm")}
						game:GetService("ReplicatedStorage"):WaitForChild("b"):FireServer(unpack(args))
					end 
				end 
			end 
		end 
	
	end 
	userinputservice.InputBegan:Connect(extendedhand)
	function bullhand(input, typing)
		if typing == true then return end
		if input.KeyCode == Enum.KeyCode.B then 
			print("Pressed B")
			while getgenv().EnabledBull == true do 
				task.wait() 
				if getgenv().EnabledBull == true then 
					local plrs = players:GetPlayers() 
					for i, v in pairs(plrs) do
						task.wait() 
						local name = v.Name 
						local args = {[1] = players[v.Name].Character:FindFirstChild("Left Leg")}
						game:GetService("ReplicatedStorage").BullHit:FireServer(unpack(args))
					end 
				end 
			end 
		end 
	end
	userinputservice.InputBegan:Connect(bullhand)
end
coroutine.wrap(RSCHG_fake_script)()
local function HDBWCVS_fake_script() -- e.toggle 
	local script = Instance.new('LocalScript', e)

	local uis = game:GetService("UserInputService")
	uis.InputBegan:Connect(function(i, z)
		if z == true then return end 
		if i.KeyCode == Enum.KeyCode.Z then 
			print("Pressed Z")
			script.Parent.Killstreak.Visible = not script.Parent.Killstreak.Visible
			script.Parent.reaper.Visible = not script.Parent.reaper.Visible 
			script.Parent.boomerang.Visible = not script.Parent.boomerang.Visible
			script.Parent["extended hand"].Visible = not script.Parent["extended hand"].Visible
			script.Parent.bullhand.Visible = not script.Parent.bullhand.Visible 
		end
	end)
end
coroutine.wrap(HDBWCVS_fake_script)()
